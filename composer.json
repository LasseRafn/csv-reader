{
	"name": "lasserafn/csv-reader",
	"description": "A minimal League/CSV wrapper. Gives the powerful CSV tool an acceptable API. Very opinionated. Used for own projects, so might not fit your case. Just use League/csv.",
	"type": "library",
	"require": {
		"php": ">=7.0",
		"league/csv": "^9.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.1",
		"satooshi/php-coveralls": "^2.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		}
	],
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "7.2"
		}
	},
	"autoload": {
		"psr-4": {
			"LasseRafn\\CsvReader\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CsvReader\\Tests\\": "tests/"
		}
	}
}
